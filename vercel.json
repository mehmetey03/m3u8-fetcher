{
  "version": 2,
  "builds": [
    {
      "src": "api/fetch.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/fetch/(.*)",
      "dest": "/api/fetch"
    },
    {
      "src": "/stream/(.*)",
      "dest": "/api/fetch"
    },
    {
      "src": "/proxy",
      "dest": "/api/fetch"
    }
  ]
}