{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/fetch/([0-9]+)",
      "dest": "/api/index?path=fetch&id=$1",
      "methods": ["GET"]
    },
    {
      "src": "/stream/([0-9]+)",
      "dest": "/api/index?path=stream&id=$1",
      "methods": ["GET"]
    },
    {
      "src": "/proxy",
      "dest": "/api/index?path=proxy",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/index?path=404"
    }
  ]
}
